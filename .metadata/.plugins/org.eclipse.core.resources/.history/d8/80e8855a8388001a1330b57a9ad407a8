package com.example.demo.feignproxy;


import java.util.List;

import org.springframework.cloud.openfeign.FeignClient;
import org.springframework.stereotype.Service;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.PathVariable;

import com.example.demo.model.DataServiceModel;



@FeignClient(name ="data-service")
@Service
public interface DataserviceProxy {
	
	@GetMapping("/requests/{empid}")
	public DataServiceModel findEmpById(@PathVariable Integer empid);
}
